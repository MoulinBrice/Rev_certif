<h1>Rooms#index</h1>
<p>Find me in app/views/rooms/index.html.erb</p>
<%= link_to "Retour à la page d'accueil", root_path %>

<h2>Liste des chambres proposées</h2>
<ul>
    <% @rooms.each do |room| %>
        <li><%= room.price_per_night %> <br>
            <%= room.capacity %> <br>
            <%= link_to "Détails de la chambre", room_path(room.id) %>
        </li>
    <% end %>
</ul>
